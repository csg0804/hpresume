.experience-wrap{display:flex;flex-direction:column;background:#fff}.experience-wrap .top-back{width:100%;height:160rpx;overflow:hidden;position:relative}.experience-wrap .top-back::after{content:"";width:400vw;height:400vw;background:#3dcaff;border-radius:50%;position:absolute;bottom:0;left:50%;transform:translateX(-50%);z-index:1}.experience-wrap .mycnt{color:#fff;text-align:center;line-height:160rpx;font-weight:bold;position:relative;z-index:2}.experience-wrap .main{flex:1;display:flex;align-items:center;overflow:auto}.experience-wrap .company{display:flex;padding:30rpx 30rpx 0}.experience-wrap .company .logo{display:block;width:100rpx;height:100rpx;border:none;margin-right:20rpx;border-radius:10rpx}.experience-wrap .company .info-part{flex:1}.experience-wrap .company .name{font-size:32rpx;color:#333;font-weight:bold;line-height:46rpx;margin-bottom:10rpx}.experience-wrap .company .job,.experience-wrap .company .time{font-size:28rpx;color:#333;line-height:36rpx}.experience-wrap .company .time{margin-bottom:10rpx}.experience-wrap .company .cominfo{font-size:26rpx;color:#666;line-height:36rpx;padding-bottom:20rpx}