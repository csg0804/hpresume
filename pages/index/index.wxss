page{background:#fff}.index-wrap{display:flex;flex-direction:column}.index-wrap .top-part{width:100%;height:350rpx;display:flex;align-items:flex-end;position:relative;overflow:hidden;background:#3dcaff}.index-wrap .top-part .title-box{width:100%;position:absolute;top:50rpx;left:0;z-index:10;text-align:left;padding-left:40rpx}.index-wrap .top-part .title-box .title{font-size:56rpx;color:#fff;font-weight:700}.index-wrap .top-part .title-box .title-info{color:#fff;font-size:26rpx}.index-wrap .top-part .title-box .nums{font-size:70rpx;font-weight:bold;color:yellow}.index-wrap .top-part .title-box .tolink{display:block;width:180rpx;height:60rpx;font-size:24rpx;color:#ff8300;background:#fff;line-height:60rpx;padding:0}.index-wrap .top-part .title-box .tolink::after{display:none}.index-wrap .top-part .icon-back{position:absolute;top:-80rpx;right:-80rpx;font-size:310rpx;color:#fff;opacity:.1;transform:rotate(90deg)}.index-wrap .top-part .right-imgs{display:block;width:350rpx;height:200rpx;position:absolute;bottom:0;right:0;opacity:.3}.index-wrap .circle-list{flex:1;display:flex;flex-wrap:wrap;justify-content:center;position:relative}.index-wrap .circle-list .circle-item{float:left;width:50%;display:flex;flex-direction:column;align-items:center;justify-content:center}.index-wrap .circle-list .circle-item .names{font-size:26rpx;color:#333;font-weight:bold;text-align:center;margin-top:20rpx}.index-wrap .circle-list .sline{width:2rpx;height:100%;position:absolute;top:0;left:50%;transform:translateX(-50%);z-index:10}.index-wrap .circle-list .hline{width:100%;height:2rpx;position:absolute;top:50%;left:0;transform:translateY(-50%);z-index:10}