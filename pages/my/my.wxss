.my-wrap{padding-top:50rpx;background:#fff;position:relative;box-sizing:border-box}.my-wrap .top-back{width:100%;height:160rpx;overflow:hidden;position:absolute;top:-1px;left:0;z-index:1}.my-wrap .top-back::after{content:"";width:400vw;height:400vw;background:#3dcaff;border-radius:50%;position:absolute;bottom:0;left:50%;transform:translateX(-50%)}.my-wrap .user-part{display:flex;width:690rpx;margin:0 auto 30rpx;padding:40rpx 30rpx;background:#fff;position:relative;z-index:5;border-radius:12rpx;box-sizing:border-box}.my-wrap .user-part .photos{display:block;width:160rpx;height:160rpx;margin-right:30rpx;border-radius:50%}.my-wrap .user-part .user-info{flex:1;display:flex;font-size:32rpx;color:#333;line-height:36rpx;flex-direction:column;justify-content:center}.my-wrap .user-part .user-info .user-name{font-size:38rpx;font-weight:700;line-height:50rpx;margin-bottom:5rpx}.my-wrap .user-part .user-info .year{font-size:24rpx;color:#999;margin-top:15rpx}.my-wrap .menues{padding:0 30rpx}.my-wrap .menues .menue-item{display:flex;width:100%;font-size:32rpx;color:#333;line-height:32rpx;font-weight:normal;text-align:left;align-items:center;padding:35rpx 0;background:#fff}.my-wrap .menues .menue-item .icons{font-size:38rpx;line-height:40rpx;margin-right:20rpx}.my-wrap .menues .menue-item .menue-name{flex:1}.my-wrap .menues .menue-item .right-arrow{font-size:24rpx;color:#a3a3a3}